<letter> ::= [CharacterSet.letters]
<digit>  ::= [CharacterSet.decimalDigits]

<keyword> ::= "var" | "func" | "extern"

<identifier>      ::= <identifier-head> <identifier-body>
<identifier-head> ::= <letter> | "_"
<identifier-body> ::= "" | <identifier-head> | <digit>

<integer-literal>      ::= <digit> <integer-literal-body>
<integer-literal-body> ::= "" | <digit> | "_"
